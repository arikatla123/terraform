    pipeline {
          agent any
          stages {
                stage('git chekou') {
                      steps {
                            echo 'Hi, this is Anshul from LevelUp360'
                       
                            
                      }
                }
                stage('Build') {
                      steps {
                            echo 'Building Sample Maven Project'
                      }
                }
                stage('Deploy') {
                      steps {
                            echo "Deploying in Staging Area"
                      }
                }
                stage('Deploy Production') {
                      steps {
                            echo "Deploying in Production Area"
                      }
                }
          }
    }
